# use glob syntax.
syntax: glob

# Ignore all files of these types:
*~
*.o
*.a
## these are temporary files for pdflatex compile
*_latexmk
*.log
*.aux
*.dvi
*.bbl
*.blg

# switch to regexp syntax.
